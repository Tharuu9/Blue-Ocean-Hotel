<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="backIncome" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="IncomeController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" layoutY="-3.0" opacity="0.43" pickOnBounds="true">
         <image>
            <Image url="@img/main%2520form/8.jpg" />
         </image>
      </ImageView>
      <Label layoutX="160.0" layoutY="14.0" prefHeight="43.0" prefWidth="283.0" style="-fx-background-color: #00FFFF;" text="Income Reports" textFill="#1d1e1d">
         <font>
            <Font name="Arial Rounded MT Bold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="btnMonthlyRep" layoutX="191.0" layoutY="128.0" mnemonicParsing="false" onAction="#OnActionMonthlyRep" prefHeight="51.0" prefWidth="239.0" style="-fx-background-color: #008B8B;" text="View Monthly ">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="btnYearlyRep" layoutX="191.0" layoutY="200.0" mnemonicParsing="false" onAction="#OnActionYearlyRep" prefHeight="51.0" prefWidth="239.0" style="-fx-background-color: #006400;" text="View Annually">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="btnBack" layoutX="14.0" layoutY="330.0" mnemonicParsing="false" onAction="#OnActionBack" style="-fx-background-color: red;" text="&lt;--Back">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
