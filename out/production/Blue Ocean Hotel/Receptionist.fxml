<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="ReceptionLog" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #B8860B;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ReceptionistController">
   <children>
      <Label layoutX="160.0" layoutY="14.0" style="-fx-background-color: #FF8C00;" text="Reception Login Page ">
         <font>
            <Font name="Berlin Sans FB" size="27.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="133.0" text="User Name  :-" textFill="#8cdd00">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="userName" alignment="TOP_LEFT" layoutX="206.0" layoutY="135.0" prefHeight="25.0" prefWidth="232.0" />
      <Label layoutX="14.0" layoutY="201.0" text="Password     :-" textFill="#4dff00">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="Password" alignment="TOP_LEFT" layoutX="206.0" layoutY="203.0" prefHeight="25.0" prefWidth="232.0" />
      <Button layoutX="268.0" layoutY="275.0" mnemonicParsing="false" onAction="#OnActionLogin" style="-fx-background-color: #FFF8DC;" text="Login" textFill="#731194">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="23.0" layoutY="343.0" mnemonicParsing="false" onAction="#OnActionBack" style="-fx-background-color: #DC143C;" text="&lt;--Back">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label fx:id="wrong" layoutX="198.0" layoutY="349.0" prefHeight="26.0" prefWidth="279.0" text="User name or password is wrong." textFill="#f20a0a" visible="false">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
